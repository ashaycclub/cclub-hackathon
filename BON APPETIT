#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,k,s=0,a[100000],b,left,i,c;
    scanf("%d %d",&n,&k);
    for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        s=s+a[i];
    }
    scanf("%d",&b);
    left=(s-a[k])/2;
    if(b==left)
        printf("Bon Appetit");
    else
    {   c=b-left;
        printf("%d",c);
    }
    return 0;
}

