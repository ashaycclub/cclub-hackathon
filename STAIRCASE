#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main()
{int n,k,c,space; 
  scanf("%d",&n);
 space = n;
  for ( k = 1 ; k <= n ; k++ )
  {
  for ( c = 1 ; c < space ; c++ )
     printf(" ");
  space--; 
 for( c = 1 ; c <= k ; c++ )
    printf("#");
    printf("\n");
    }
    return 0;
}

